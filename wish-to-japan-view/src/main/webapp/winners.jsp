	</script>

	<script type="text/javascript">
		var fujifilmWinnerQtyHolder = $("#fujifilm-winner-qty");
		var japanWinnerQtyHolder = $("#japan-winner-qty");
		var lenovoWinnerQtyHolder = $("#lenovo-winner-qty");
		var macbookWinnerQtyHolder = $("#macbook-winner-qty");
		var meizuWinnerQtyHolder = $("#meizu-winner-qty");
		
		var fujifilmListHolder = $("#fujifilm-winner-list");
		var japanListHolder = $("#japan-winner-list");
		var lenovoListHolder = $("#lenovo-winner-list");
		var macbookListHolder = $("#macbook-winner-list");
		var meizuTableHolder = $("#meizu-winner-table");
		var meizuLeftColumnTableHolder = $("#meizu-left-column-table");
		var meizuRightColumnTableHolder = $("#meizu-right-column-table");
		
		function fillList(dataViewListHolder, dataViewQtyHolder, dataList) {
			var innerHTML = "";
			for (i = 0; i < dataList.length; i++) {
				innerHTML += "<li>" + dataList[i].name + " <span>" + dataList[i].voucherCode + "</span></li>";
			}
			dataViewListHolder.html(innerHTML);
			dataViewQtyHolder.html(dataList.length);
		}
		
		function fillMeizuTable(dataList) {
			var toggler = "L";
			
			meizuWinnerQtyHolder.html(dataList.length);
			
			for (i = 0; i < dataList.length; i++) {
				var innerHTML = "<tr class='tr-blok'><td>" + dataList[i].name + "</td><td class='right'>" + dataList[i].voucherCode + "</td></tr>";
				
				if (toggler == "L") {
					toggler = "R";
					meizuLeftColumnTableHolder.append(innerHTML);
				} else {
					toggler = "L";
					meizuRightColumnTableHolder.append(innerHTML);
				}
			}
		}
		
		$(document).ready(function() {
			$(".loading").html("LOADING...");
			$.ajax({
				method: 'get',
				url: "/api/show-winners"
			}).done(function(data) {
				if (!data.valid) {
					window.location =  "//" +  viewHost + "/index.jsp";
				}
				var fujifilmWinners = data.fujifilmWinners;
				var japanWinners = data.japanWinners;
				var lenovoWinners = data.lenovoWinners;
				var macbookWinners = data.macbookWinners;
				var meizuWinners = data.meizuWinners;
				
				fillList(fujifilmListHolder, fujifilmWinnerQtyHolder, fujifilmWinners);
				fillList(japanListHolder, japanWinnerQtyHolder, japanWinners);
				fillList(lenovoListHolder, lenovoWinnerQtyHolder, lenovoWinners);
				fillList(macbookListHolder, macbookWinnerQtyHolder, macbookWinners);
				fillMeizuTable(meizuWinners);
				
				$(".loading").css("display","none");
			}).error(function() {
				swal({
				  title: "Error!",
				  text: "Maaf, data pemenang tidak berhasil dimuat. Silakan tekan F5 atau Ctrl+R pada keyboard Anda apabila halaman tidak berubah.",
				  type: "error",
				  confirmButtonText: "Close",
				});
			});
		});
	</script>
